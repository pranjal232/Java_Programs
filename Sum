public class Sum{
    public static void main(String[] args) {

        int num1 = 2;
        int num2 = 4;
        int sum = num1 + num2;
        System.out.println(num1);
        System.out.println(num2);
        System.out.print("Sum of these number = ");
        System.out.println(sum);
    }
}
